{
    "name": "magdv/cargo-php-api-client",
    "type": "library",
    "description": "Cargomart.ru API client",
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "php": ">=7.4",
        "ext-mbstring": "*",
        "ext-json": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Bragin",
            "email": "i.bragin@magdv.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Cargomart\\ApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cargomart\\ApiClient\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.18",
        "mikey179/vfsstream": "^1.6",
        "phpstan/phpstan": "^0.12.38",
        "phpunit/phpunit": "^9.3",
        "psr/simple-cache": "^1.0"
    },
    "config": {
        "sort-packages": true
    },
    "suggest": {
        "psr/simple-cache": "Используется для хранения сессий"
    }
}
